{
  "name": "maxquebral/laravel-doctrine-factory-generator",
  "description": "Doctrine entity factory generator for testing",
  "authors": [
    {
      "name": "Max Quebral",
      "email": "maxquebral@gmail.com"
    }
  ],
  "keywords": [
    "laravel",
    "doctrine",
    "entity",
    "factory"
  ],
  "license": "MIT",
  "require": {
    "illuminate/support": "^5.8",
    "illuminate/filesystem": "^5.8",
    "illuminate/console": "^5.8",
    "fzaninotto/faker": "^1.8"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.2",
    "laravel/lumen-framework": "^5.8",
    "laravel-doctrine/orm": "^1.4",
    "mockery/mockery": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "PhpCircle\\FactoryGenerator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\PhpCircle\\FactoryGenerator\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "PhpCircle\\FactoryGenerator\\FactoryGeneratorServiceProvider"
      ]
    }
  }
}
